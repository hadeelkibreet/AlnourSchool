<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hadilKibreet.alnour">

    <!-- The INTERNET permission is required for development. Specifically,
         the Flutter tool needs it to communicate with the running application
         to allow setting breakpoints, to provide hot reload, etc.
    -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="your_target_sdk_version_here"
        tools:overrideLibrary="io.flutter.plugins.firebase.core, io.flutter.plugins.firebase.auth" />
    <application
        android:label="Alnour"
        tools:replace="android:label"
        android:icon="@mipmap/ic_launcher">

        <!-- Add your activities, services, etc. here -->

    </application>

    </manifest>